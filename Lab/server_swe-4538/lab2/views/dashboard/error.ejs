<%- include('../partials/header.ejs') %>

<section class="bg-white py-28">
  <div class="py-6 px-3 mx-auto max-w-screen-xl lg:py-8 lg:px-4">
    <div class="mx-auto max-w-screen-sm text-center">
      <h1
        class="mb-2 text-5xl tracking-tight font-extrabold lg:text-7xl text-primary-600"
      >
        <%= status %>
      </h1>
      <p
        class="mb-2 text-xl tracking-tight font-semibold text-gray-900 md:text-2xl"
      >
        <%= message %>
      </p>
      <% if (error) { %>
      <pre class="mb-2 text-base font-light text-gray-500">
        <%= error.stack %>
      </pre>
      <% } %>
      <a
        href="/dashboard"
        class="inline-flex text-white bg-primary-600 hover:bg-primary-800 focus:ring-4 focus:outline-none focus:ring-primary-300 font-medium rounded-lg text-xs px-4 py-2 text-center my-2"
      >
        Back to Dashboard
      </a>
    </div>
  </div>
</section>
